{
    "version": 2,
    "builds": [
        {
            "src": "/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods":["GET","POST","PUT","PACTH","DELETE"],
            "dest": "/src/index.js"
        }
    ]
}
